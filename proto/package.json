{
  "name": "proto",
  "version": "0.0.1",
  "description": "prototype",
  "main": "index.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "start": "webpack-dev-server",
    "build": "webpack --env.production",
    "build:dev": "webpack",
    "rebuild": "npm-run-all -s clean lint build",
    "rebuild:dev": "npm-run-all -s clean build:dev",
    "lint": "tslint 'src/**/*.ts'",
    "lint:all": "tslint 'src/**/*.ts' 'src/**/*.vue'",
    "unit": "jest",
    "unitw": "jest --watch",
    "unitc": "jest --coverage",
    "tsc": "tsc"
  },
  "author": "Yoshiaki Itakura (https://github.com/negibouze)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.4.6",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-connect": "^2.1.0"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.30.11",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@vue/test-utils": "^1.0.0-beta.24",
    "autoprefixer": "^9.1.1",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "browserslist": "^4.0.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.8",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-serializer-vue": "^2.0.2",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.22.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.3.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "jest": {
    "globals": {
      "vue-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      },
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/unit/setup.ts",
      "<rootDir>/__tests__/unit/coverage"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/unit/setup.ts"
    ],
    "coverageDirectory": "<rootDir>/__tests__/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/router/index.js",
      "!**/node_modules/**"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 0.25% in JP",
              "not op_mini all"
            ]
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      [
        "component",
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  }
}
