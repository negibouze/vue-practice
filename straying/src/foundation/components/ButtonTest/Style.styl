.btn-normal
  width: 120px
  height: 40px
  border: 1px solid transparent
  border-radius: 4px
  background-color: #fcfcfc
  color: #292b2c
  font-size: inherit
  transition-property: all
  transition-duration: 200ms
  transition-timing-function: ease-in-out
  &:hover
    border-color: #adadad
    background-color: #e4e4e4
