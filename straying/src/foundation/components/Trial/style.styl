.container
  width: 100%
  height: 100%
.info-container
.img-container
  width: 50%
  height: 50%
.list-container
  width: 100%

table
  color: #333
  font-family: Helvetica, Arial, sans-serif
  width: 640px 
  border-collapse: collapse
  border-spacing: 0 

td, th
  border: 1px solid #CCC
  height: 30px

th
  background: #F3F3F3
  font-weight: bold

td
  background: #FAFAFA
  text-align: center
